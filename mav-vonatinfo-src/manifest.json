{
  "manifest_version": 3,
  "name": "Mav vonatinfo",
  "version": "1.0",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_title": "Megnyit√°s"
  },
 "host_permissions": ["*://*/*"],
  "web_accessible_resources": [
    {
      "resources": ["inject.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
